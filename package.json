{
  "name": "solid-valtio",
  "version": "0.1.8",
  "description": "ðŸ’Š State management in Solid with valtio.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-valtio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-valtio.git"
  },
  "keywords": [
    "solid",
    "state",
    "manager",
    "management",
    "mobx",
    "proxy",
    "store"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vanilla": {
      "types": "./dist/vanilla.d.ts",
      "require": "./dist/vanilla.js",
      "import": "./dist/vanilla.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "solid-js": ">=1.4.0"
  },
  "dependencies": {
    "klona": "^2.0.5",
    "valtio": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.16.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "pnpm": "^7.3.0",
    "rollup": "^2.58.0",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.5",
    "tsup": "^6.1.3",
    "typescript": "^4.4.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
