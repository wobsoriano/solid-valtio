{
  "name": "solid-valtio",
  "type": "module",
  "version": "0.2.1",
  "description": "ðŸ’Š State management in Solid with valtio.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-valtio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-valtio.git"
  },
  "keywords": [
    "solid",
    "state",
    "manager",
    "management",
    "mobx",
    "proxy",
    "store"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0",
    "valtio": "^1.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "bumpp": "^9.4.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.21.3",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^24.0.0",
    "prettier": "3.0.0",
    "solid-js": "^1.8.17",
    "tsup": "^8.0.2",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.4.5",
    "valtio": "^1.13.2",
    "vite": "^5.2.11",
    "vite-plugin-solid": "^2.10.2"
  },
  "browser": {}
}
